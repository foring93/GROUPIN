<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="Prs">

    <select id="count" resultType="int">
        select count(*) from PrBoard 
    </select>

	<select id="boardList" parameterType="java.util.Map" resultType="prboard">
	  <![CDATA[	select * from PrBoard order by prKey desc ]]>
	</select>
	
<!-- select * from (select * rownum rnum, p.* from (select * from PrBoard order by prKey desc) p) where rnum >= #{start} and rnum <= #{end} -->

    <insert id="insert" parameterType="prboard">
        insert into PrBoard(
           prKey, userKey, groupKey, content, datewrite
        )    
        values (
           #{prKey}, #{userKey}, #{groupKey}, #{content}, sysdate
        )
    </insert>


</mapper>